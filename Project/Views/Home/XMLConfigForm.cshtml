@{
    Layout = "~/Views/Home/_Home_Layout.cshtml";
    ViewBag.Title = "GOMC: XML";
}
@section NavSection {
    <li>@Html.ActionLink("GOMC", "Gomc", "Home")</li>
    <li>@Html.ActionLink("Features", "Features", "Home")</li>
    <li>@Html.ActionLink("Downloads", "Downloads", "Home")</li>
    <li>@Html.ActionLink("Documentation", "Documentation", "Home", htmlAttributes: new { id = "onDocumentationPage" })</li>
    <li>@Html.ActionLink("Publications", "Publications", "Home")</li>
    <li>@Html.ActionLink("About", "About", "Home")</li>
}
<main>
        <div class="row">
            <ul id="progressbar">
                <li class="active">Structure</li>
                <li>System</li>
                <li>Config Header</li>
                <li>Config Body</li>
            </ul>
            <div class='col-md-12 well well-lg ' id=xmlDataContainer>
           
                @*<div class="progress">
                    <div class='progress-bar bar-style' role='progressbar' aria-valueactive='1' aria-valuemin='0' aria-valuemax="8" id='userProgress'>
                        <span id="progressStatus"></span>
                    </div>
                </div>*@
                    <div class='panel-group '>
                        <div class="panel panel-success panel-first working-panel">
                            <div class="panel-heading">Initial Structure Setup</div>
                            <div class="panel-body">
                                <form id="xmlForm1">
                                    <h4>Please select an Ensemble</h4>
                                    <label>Ensembles</label>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Canonical Ensemble" data-placement="right" >
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Ensemble" id="Nvt" value="Nvt">
                                            NVT
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Isobaric-isothermal Ensemble" data-placement="right">
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Ensemble" id="Npt" value="Npt">
                                            NPT
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Grand Canonical Ensemble" data-placement="right">
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Ensemble" id="Gcmc" value="Gcmc">
                                            GCMC
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Constant Volume Gibbs Ensemble" data-placement="right">
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Ensemble" id="GibbsNvt" value="GibbsNvt">
                                            GibbsNVT
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Constant Pressure Gibbs Ensemble" data-placement="right">
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Ensemble" id="GibbsNpt" value="GibbsNpt">
                                            GibbsNpt
                                        </label>
                                    </div>
                                    <h4>Please enter the following input data</h4>
                                    <label data-toggle="tooltip" title="Determines whether to restart the simulation">Restart</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Restart" id="Restart-true" value="true">
                                            True
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Restart" id="Restart-false" value="false">
                                            False
                                        </label>
                                    </div>
                                    <label data-toggle="tooltip" title="Dictates how to start the pseudo-random number generator (random string or random numbers)">Prng</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Prng" id="PrngRandom" value="Random">RANDOM
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Prng" id="PrngIntseed" value="Intseed">INTSEED
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Sets the random int seed, only if Prng = INTSEED">RandomSeed</label>
                                        <input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_RandomSeed" id="RandomSeed">
                                    </div>
                                    <label data-toggle="tooltip" title="Sets force field type">ParaType</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_ParaType" id="ParaTypeCharmm" value="Charmm">
                                            CHARMM
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_ParaType" id="ParaTypeExotic" value="Exotic">
                                            MIE
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_ParaType" id="ParaTypeMartini" value="Martini">
                                            MARTINI
                                        </label>
                                    </div>
                                    <h4>Please enter the input configuration data</h4>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Provides the name and location of the parameter file to use for the simulation">ParametersFileName</label>
                                        <input class="xml-control form-control" type="text" name="gomc_config_input_ParametersFileName" id="ParametersFileName">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Define filenames for each box in the system">Coordinate File (<i>.pdb</i>)</label>
                                        <input class="xml-control form-control" type="text"   name="gomc_config_input_Coordinates_1" id="InputCoordinates1">
                                        <input class="xml-control form-control" type="text"   name="gomc_config_input_Coordinates_2" id="InputCoordinates2">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Define the PSF filenames for each box in the system">Structure File (<i>.psf</i>)</label>
                                        <input class="xml-control form-control" type="text" pattern="[\s-]" name="gomc_config_input_Structures_1" id="InputStructures1">
                                        <input class="xml-control form-control" type="text" pattern="[\s-]" name="gomc_config_input_Structures_2" id="InputStructures2">
                                    </div>
                                </form>
                                <button class="btn btn-success btn-sm pull-left" id="xmlForm1Save"><span class="glyphicon glyphicon-arrow-right"></span>  Next</button>
                        </div>
                        </div>
                        <div class="panel panel-success panel-second not-render">
                            <div class="panel-heading">Initial System Setup</div>
                            <div class="panel-body">
                                <h4>Enter the following System Data</h4>
                                <form id="xmlForm2">
                                    <div class="form-group">
                                        <div>
                                            <label data-toggle="tooltip" title="NPT or GibbsNpt must be selected, Pressure is in Bar">Pressure</label>
                                        </div>
                                        <input class="xml-control form-control inline-control" type="number" step="any" min="0" name="gomc_config_input_Pressure" id="Pressure"><span>  <i>Bar</i></span>
                                    </div>
                                    <div class="form-group">
                                        <div>
                                            <label data-toggle="tooltip" title="Sets temperature for system">Temperature</label>
                                        </div>
                                        <input class="xml-control form-control inline-control" type="number" step="any" min="0" name="gomc_config_input_Temperature" id="Temperature"><span>  <i>Kelvin</i></span>
                                    </div>
                                    <div class="form-group">
                                        <div>
                                            <label data-toggle="tooltip" title="Specific radius of non-bonded interaction energy">Rcut</label>
                                        </div>
                                        <input class="xml-control form-control inline-control" type="number" step="any" min="0" name="gomc_config_input_Rcut" id="Rcut"><span>  <i>Angstrom</i></span>
                                    </div>
                                    <div class="form-group">
                                        <div>
                                            <label data-toggle="tooltip" title="Sets minimal possible distance">RcutLow</label>
                                        </div>
                                        <input class="xml-control form-control inline-control" type="number" step="any" min="0" name="gomc_config_input_RcutLow" id="RcutLow"><span>  <i>Angstrom</i></span>
                                    </div>
                                    <label data-toggle="tooltip" title="Defines whether or not long range corrections are used">Long Range Correction</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Lrc" id="InputLrc-true" value="true">
                                            True
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Lrc" id="Lrc-false" value="false">
                                            False
                                        </label>
                                    </div>
                                    <label data-toggle="tooltip" title="Define which pairs of bonded atoms should be excluded from non-bonded interactions">Exclude Interactions</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Exclude" id="Exclude-OneTwo" value="OneTwo">
                                            1-2
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Exclude" id="Exclude-OneThree" value="OneThree">
                                            1-3
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Exclude" id="Exclude-OneFour" value="OneFour" checked>
                                            1-4
                                        </label>
                                    </div>
                                    <label data-toggle="tooltip" title="Define potential function type to calculate non-bonded energy between atoms">Lennard-Jones</label>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Nonbonded interaction energy and force based on the Lennard Johns n-6 equation">
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Potential" id="PotentialVdw" value="Vdw">
                                            Truncated
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Option forces the energy to be zero at Rcut distance">
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Potential" id="PotentialShift" value="Shift">
                                            SHIFT
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label data-toggle="tooltip" title="Option forces potential energy to be zero at Rcut distance. Starts modification at RSwitch distance">
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Potential" id="PotentialSwitch" value="Switch">
                                            SWITCH
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Switch must be the potential function. A distance is non-bonded and interaction energy is truncated smoothly">Rswitch</label>
                                        <input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_Rswitch" id="RSwitch">
                                    </div>
                                    <h4>Enter metadata</h4>
                                    <label data-toggle="tooltip" title="Consider Coulomb interaction or not">ElectroStatic</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_ElectroStatic" id="ElectroStatic-true" value="true">
                                            True
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_ElectroStatic" id="ElectroStatic-false" value="false">
                                            False
                                        </label>
                                    </div>
                                    <label data-toggle="tooltip" title="Considers summation for electrostatic calculation">Ewald</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Ewald" id="Ewald-true" value="true">
                                            True
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_Ewald" id="Ewald-false" value="false">
                                            False
                                        </label>
                                    </div>
                                    <label data-toggle="tooltip" title="Considers storing the reciprocal for Ewald summation">CachedFourier</label>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_CachedFourier" id="CachedFourier-true" value="true">
                                            True
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="xml-control form-check-input" type="radio" name="gomc_config_input_CachedFourier" id="CachedFourier-false" value="false">
                                            False
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Specifies accuracy of the Ewald summation calculation">Tolerance</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_Tolerance" id="Tolerance">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Defines dielectric constant for MARTINI force field">Dielectric</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_Dielectric" id="Dielectric">
                                    </div>
                                    <div class="form-group">
                                        <div>
                                            <label data-toggle="tooltip" title="Considers pressure calculation or not">PressureCalc</label>
                                        </div>
                                        <input class="xml-control form-control inline-control" type="number" min="0" name="gomc_config_input_PressureCalc" id="PressureCalc"><span><i> Bar</i></span>
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Defines constant factor to modify 1-4 intra-molecule coulomb interaction">1-4 Scaling</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_OneFourScaling" id="OneFourScaling">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Sets the total number of steps to run">RunSteps</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_RunSteps" id="RunSteps">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Sets the number of steps necessary to equilibrate the system">EqSteps</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_EqSteps" id="EqSteps">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Sets the number of steps per adjustment to the maximum constants for each move">AdjSteps</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_AdjSteps" id="AdjSteps">
                                    </div>
                                    <label data-toggle="tooltip" title="For GCMC chemical potential at which simulation is run">ChemPot</label>
                                    <div class="form-group">
                                        <label>ResName<input class="xml-control form-control" type="text" pattern="[\s-]" name="gomc_config_input_ChemPot_ResName" id="ChemPotResName"></label>
                                        <label>Value<input class="xml-control form-control" type="number" min="0" name="gomc_config_input_ChemPot_Value" id="ChemPotValue"></label>
                                    </div>
                                    <label data-toggle="tooltip" title="For GCMC Fugacity at which the simulation is run">Fugacity</label>
                                    <div class="form-group">
                                        <label>ResName<input class="xml-control form-control" type="text" pattern="[\s-]" name="gomc_config_input_Fugacity_ResName" id="FugacityResName"></label>
                                        <label>Value<input class="xml-control form-control" type="number" min="0" name="gomc_config_input_Fugacity_Value" id="FugacityValue"></label>
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Fractional percentage at which displacement move will occur">DisFreq</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_DisFreq" id="DisFreq">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Fractional % for rigid rotation">RotFreq</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_RotFreq" id="RotFreq">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="Fractional % at which the particle will be removed from and inserted to the same box">IntraSwapFreq</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_IntraSwapFreq" id="IntraSwapFreq">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title=" For Gibbs and GCMC, Fractional % at which volume displacement move will occur">VolFreq</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_VolFreq" id="VolFreq">
                                    </div>
                                    <div class="form-group">
                                        <label data-toggle="tooltip" title="For Gibbs and GCMC fractional percentage at which the particle swap move will occur">SwapFreq</label>
                                        <input class="xml-control form-control" type="number" min="0" name="gomc_config_input_SwapFreq" id="SwapFreq">
                                    </div>
                                    <button class="btn btn-default btn-sm pull-left prev-btn" formnovalidate><span class="glyphicon glyphicon-arrow-left"></span>  Prev</button>
                                    <button class="btn btn-success btn-sm pull-left" id="xmlForm2Save"><span class="glyphicon glyphicon-arrow-right"></span>  Next</button>
                                </form>
                            </div>
                        </div>
                        <div class='panel panel-success panel-third not-render'>
                            <div class='panel-heading'>Output Header Configuration</div>
                            <div class='panel-body '>
                                <h4>Enter Metadata Configuration Data</h4>
                               <form id="xmlForm3">
                                   <label data-toggle="tooltip" title="For Gibbs, consider change to the volume of the simulation box">Use Constant Area</label>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_UseConstantArea" id="UseConstantArea-true" value="true">
                                           True
                                       </label>
                                   </div>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_UseConstantArea" id="UseConstantArea-false" value="false">
                                           False
                                       </label>
                                   </div>
                                   <label data-toggle="tooltip" title="For absorption in GibbsNPT changing the volume of the fluid phase box to maintain imposed pressure and temperature">FixVolBox0</label>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_FixVolBox0" id="FixVolBox0-true" value="true">
                                           True
                                       </label>
                                   </div>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_FixVolBox0" id="FixVolBox0-false" value="false">
                                           False
                                       </label>
                                   </div>
                                   <div class="form-group">
                                       <label data-toggle="tooltip" title="Defines the axis of the simulation box, it occurs once for NVT & NPT twice for Gibbs or GC">BoxDim 0</label>
                                       <label>XAxis<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_1_XAxis" id="BoxDim1-XAxis"></label>
                                       <label>YAxis<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_1_YAxis" id="BoxDim1-YAxis"></label>
                                       <label>ZAxis<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_1_ZAxis" id="BoxDim1-ZAxis"></label>
                                   </div>
                                   <div class="form-group">
                                       <label data-toggle="tooltip" title="Defines the axis of the simulation box, it occurs once for NVT & NPT twice for Gibbs or GC">BoxDim 1</label>
                                       <label>XAxis<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_2_XAxis" id="BoxDim2-XAxis"></label>
                                       <label>YAxis<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_2_YAxis" id="BoxDim2-YAxis"></label>
                                       <label>ZAxis<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_2_ZAxis" id="BoxDim2-ZAxis"></label>
                                   </div>
                                   <div class="form-group">
                                       <label data-toggle="tooltip" title="Number of CBMC trials to choose the first seed position">CBMC_First</label>
                                       <input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_CbmcFirst" id="CbmcFirst">
                                   </div>
                                   <div class="form-group">
                                       <label data-toggle="tooltip" title="Number of CBMC trials to choose the later seed positions">CBMC_Nth</label>
                                       <input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_CbmcNth" id="CbmcNth">
                                   </div>
                                   <div class="form-group">
                                       <label data-toggle="tooltip" title="Number of CBMC bending angle trials to perform the geometry">CBMC_Ang</label>
                                       <input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_CbmcAng" id="CbmcAng">
                                   </div>
                                   <div class="form-group">
                                       <label data-toggle="tooltip" title="Number of CBMC dihedral angle trials to perform for geometry">CBMC_Dih</label>
                                       <input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_CbmcDih" id="CbmcDih">
                                   </div>
                                   <h4>Please Enter Output Header Format</h4>
                                   <div class="form-group">
                                       <label data-toggle="tooltip" title="Unique name for the simulation used to name the block average">OutputName</label>
                                       <input class="xml-control form-control" type="text" name="gomc_config_input_OutputName" id="OutputName">
                                   </div>
                                   <label data-toggle="tooltip" title="Controls output of PDB file (coordinates)">CoordinatesFreq</label>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_CoordinatesFreq_Enabled" id="CoordinatesFreqEnabled-true" value="true">
                                           True
                                       </label>
                                   </div>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_CoordinatesFreq_Enabled" id="CoordinatesFreqEnabled-false" value="false">
                                           False
                                       </label>
                                   </div>
                                   <div class="form-group">
                                       <label>Value<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_CoordinatesFreq_Value" id="CoordinatesFreqValue"></label>
                                   </div>
                                   <label data-toggle="tooltip" title="Controls the output of the last state of the simulation">RestartFreq</label>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_RestartFreq_Enabled" id="RestartFreqEnabled-true" value="true">
                                           True
                                       </label>
                                   </div>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_RestartFreq_Enabled" id="RestartFreqEnabled-false" value="false">
                                           False
                                       </label>
                                   </div>
                                   <div class="form-group">
                                       <label>Value<input class="xml-control form-control" type="number" step="any" min="0" name="gomc_config_input_RestartFreq_Value" id="RestartFreqValue"></label>
                                   </div>
                                   <label data-toggle="tooltip" title="Controls the output to the console">ConsoleFreq</label>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_ConsoleFreq_Enabled" id="ConsoleFreqEnabled-true" value="true">
                                           True
                                       </label>
                                   </div>
                                   <div class="radio">
                                       <label>
                                           <input class="xml-control form-check-input" type="radio" name="gomc_config_input_ConsoleFreq_Enabled" id="ConsoleFreqEnabled-false" value="false">
                                           False
                                       </label>
                                   </div>
                                   <div class="form-group">
