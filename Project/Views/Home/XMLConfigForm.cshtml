@{
    Layout = "~/Views/Home/_Home_Layout.cshtml";
    ViewBag.Title = "GOMC: XML";
}
@section NavSection {
    <li>@Html.ActionLink("GOMC", "Gomc", "Home")</li>
    <li>@Html.ActionLink("Features", "Features", "Home")</li>
    <li>@Html.ActionLink("Downloads", "Downloads", "Home")</li>
    <li>@Html.ActionLink("Documentation", "Documentation", "Home", htmlAttributes: new { id = "onDocumentationPage" })</li>
    <li>@Html.ActionLink("Publications", "Publications", "Home")</li>
    <li>@Html.ActionLink("About", "About", "Home")</li>
}
<header>
    <h1>XMLConfigForm</h1>
</header>
<main>
    <!--Feed me tool-tips and validation stuff-->
    <div class="container-fluid">
        <div class="row">
            <div class='col-md-12 well well-lg ' id=xmlDataContainer>
                <div class="progress">
                    <div class='progress-bar progress-bar-striped active bar-style' role='progressbar' aria-valueactive='1' aria-valuemin='0' aria-valuemax="8" id='userProgress' style="width: 60px">
                    </div>
                </div>
                <form id="xmlConfig">
                    <div class='panel-group '>
                        <div class="panel panel-success panel-first working-panel">
                            <div class="panel-heading">Initial Structure Setup</div> <!--Maybe include pop-overs on the headers?-->
                            <div class="panel-body">
                                <h4 data-toggle="tooltip" title="Shhh">Please select an Ensemble</h4><!--Needs to be on the labels with  a description of the parameter-->
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_Nvt" value="Nvt">
                                        Nvt
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_Npt" value="Npt">
                                        Npt
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_Gcmc" value="Gcmc">
                                        Gcmc
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_GibbsNvt" value="GibbsNvt">
                                        GibbsNvt
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_GibbsNpt" value="GibbsNpt">
                                        GibbsNpt
                                    </label>
                                </div>
                                <h4>Please enter the following input data</h4>
                                <label>Restart</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Restart" id="gomc_config_input_Restart_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Restart" id="gomc_config_input_Restart_false" value="false">
                                        False
                                    </label>
                                </div>
                                <label>Prng</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Prng" id="gomc_config_input_Prng_Random" value="Random">RANDOM
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Prng" id="gomc_config_input_Prng_Intseed" value="Intseed">INTSEED
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>RandomSeed</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_RandomSeed" id="gomc_config_input_RandomSeed">
                                </div>
                                <label>ParaType</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_ParaType" id="gomc_config_input_ParaType_Charmm" value="Charmm">
                                        CHARMM
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_ParaType" id="gomc_config_input_ParaType_Exotic" value="Exotic">
                                        MIE
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_ParaType" id="gomc_config_input_ParaType_Martini" value="Martini">
                                        MARTINI
                                    </label>
                                </div>
                            <h4>Please enter the input configuration data</h4>
                            <div class="form-group">
                                <label>ParametersFileName</label>
                                <input class="form-control" type="text" pattern="[\s-]" name="gomc_config_input_ParametersFileName" id="gomc_config_input_ParametersFileName">
                            </div>
                            <div class="form-group">
                                <label>Coordinate File (<i>.pdb</i>)</label>
                                <input class="form-control" type="number" step="any" min="0" pattern="[\s-]" name="gomc_config_input_Coordinates_1" id="gomc_config_input_Coordinates_1">
                                <input class="form-control" type="number" step="any" min="0" pattern="[\s-]" name="gomc_config_input_Coordinates_2" id="gomc_config_input_Coordinates_2">
                            </div>
                            <div class="form-group">
                                <label>Structure File (<i>.psf</i>)</label>
                                <input class="form-control" type="text" pattern="[\s-]" name="gomc_config_input_Structures_1" id="gomc_config_input_Structures_1">
                                <input class="form-control" type="text" pattern="[\s-]" name="gomc_config_input_Structures_2" id="gomc_config_input_Structures_2">
                            </div>
                                <button class="btn btn-success btn-sm pull-right next-btn">Next</button>
                        </div>
                        </div>
                        <div class="panel panel-success panel-second not-render">
                            <div class="panel-heading">Initial System Setup</div>
                            <div class="panel-body">
                                <h4>Enter the following System Data</h4>
                                <div class="form-group">
                                    <label>Pressure</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_Pressure" id="gomc_config_input_Pressure"><span>Bar</span>
                                </div>
                                <div class="form-group">
                                    <label>Temperature</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_Temperature" id="gomc_config_input_Temperature"><span>Kelvin</span>
                                </div>
                                <div class="form-group">
                                    <label>Rcut</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_Rcut" id="gomc_config_input_Rcut"><span>Angstrom</span>
                                </div>
                                <div class="form-group">
                                    <label>RcutLow</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_RcutLow" id="gomc_config_input_RcutLow"><span>Angstrom</span>
                                </div>
                                <label>Long Range Correction</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Lrc" id="gomc_config_input_Lrc_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Lrc" id="gomc_config_input_Lrc_false" value="false">
                                        False
                                    </label>
                                </div>
                                <label>Exclude Intraction</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Exclude" id="gomc_config_input_Exclude_OneTwo" value="OneTwo">
                                        1-2
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Exclude" id="gomc_config_input_Exclude_OneThree" value="OneThree">
                                        1-3
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Exclude" id="gomc_config_input_Exclude_OneFour" value="OneFour">
                                        1-4
                                    </label>
                                </div>
                                <label>Lennard-Jones</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Potential" id="gomc_config_input_Potential_Vdw" value="Vdw">
                                        Truncated
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Potential" id="gomc_config_input_Potential_Shift" value="Shift">
                                        SHIFT
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Potential" id="gomc_config_input_Potential_Switch" value="Switch">
                                        SWITCH
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>Rswitch</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_Rswitch" id="gomc_config_input_Rswitch">
                                </div>
                                <h4>Enter metadata</h4>
                                <label>ElectroStatic</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_ElectroStatic" id="gomc_config_input_ElectroStatic_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_ElectroStatic" id="gomc_config_input_ElectroStatic_false" value="false">
                                        False
                                    </label>
                                </div>
                                <label>Ewald</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Ewald" id="gomc_config_input_Ewald_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_Ewald" id="gomc_config_input_Ewald_false" value="false">
                                        False
                                    </label>
                                </div>
                                <label>CachedFourier</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_CachedFourier" id="gomc_config_input_CachedFourier_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_CachedFourier" id="gomc_config_input_CachedFourier_false" value="false">
                                        False
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>Tolerance</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_Tolerance" id="gomc_config_input_Tolerance">
                                </div>
                                <div class="form-group">
                                    <label>Dielectric</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_Dielectric" id="gomc_config_input_Dielectric">
                                </div>
                                <div class="form-group">
                                    <label>PressureCalc</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_PressureCalc" id="gomc_config_input_PressureCalc"><span>Bar</span>
                                </div>
                                <div class="form-group">
                                    <label>1-4scaling</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_OneFourScaling" id="gomc_config_input_OneFourScaling">
                                </div>
                                <div class="form-group">
                                    <label>RunSteps</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_RunSteps" id="gomc_config_input_RunSteps">
                                </div>
                                <div class="form-group">
                                    <label>EqSteps</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_EqSteps" id="gomc_config_input_EqSteps">
                                </div>
                                <div class="form-group">
                                    <label>AdjSteps</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_AdjSteps" id="gomc_config_input_AdjSteps">
                                </div>
                                <label>ChemPot</label>
                                <div class="form-group">
                                    <label>ResName<input class="form-control" type="text" pattern="[\s-]" name="gomc_config_input_ChemPot_ResName" id="gomc_config_input_ChemPot_ResName"></label>
                                    <label>Value<input class="form-control" type="number" min="0" name="gomc_config_input_ChemPot_Value" id="gomc_config_input_ChemPot_Value"></label>
                                </div>
                                <label>Fugacity</label>
                                <div class="form-group">
                                    <label>ResName<input class="form-control" type="text" pattern="[\s-]" name="gomc_config_input_Fugacity_ResName" id="gomc_config_input_Fugacity_ResName"></label>
                                    <label>Value<input class="form-control" type="number" min="0" name="gomc_config_input_Fugacity_Value" id="gomc_config_input_Fugacity_Value"></label>
                                </div>
                                <div class="form-group">
                                    <label>DisFreq</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_DisFreq" id="gomc_config_input_DisFreq">
                                </div>
                                <div class="form-group">
                                    <label>RotFreq</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_RotFreq" id="gomc_config_input_RotFreq">
                                </div>
                                <div class="form-group">
                                    <label>IntraSwapFreq</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_IntraSwapFreq" id="gomc_config_input_IntraSwapFreq">
                                </div>
                                <div class="form-group">
                                    <label>VolFreq</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_VolFreq" id="gomc_config_input_VolFreq">
                                </div>
                                <div class="form-group">
                                    <label>SwapFreq</label>
                                    <input class="form-control" type="number" min="0" name="gomc_config_input_SwapFreq" id="gomc_config_input_SwapFreq">
                                </div>
                                <button class="btn btn-success btn-sm pull-left prev-btn">Previous</button>
                                <button class="btn btn-success btn-sm pull-right next-btn">Next</button>
                            </div>
                        </div>
                        <div class='panel panel-success panel-third not-render'>
                            <div class='panel-heading'>Output Header Configuration</div>
                            <div class='panel-body '>
                                <h4>Enter Metadata Configuration Data</h4>
                                <label>UseConstantArea</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_UseConstantArea" id="gomc_config_input_UseConstantArea_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_UseConstantArea" id="gomc_config_input_UseConstantArea_false" value="false">
                                        False
                                    </label>
                                </div>
                                <label>FixVolBox0</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_FixVolBox0" id="gomc_config_input_FixVolBox0_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_FixVolBox0" id="gomc_config_input_FixVolBox0_false" value="false">
                                        False
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>BoxDim 0</label>
                                    <label>XAxis<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_1_XAxis" id="gomc_config_input_BoxDim_1_XAxis"></label>
                                    <label>YAxis<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_1_YAxis" id="gomc_config_input_BoxDim_1_YAxis"></label>
                                    <label>ZAxis<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_1_ZAxis" id="gomc_config_input_BoxDim_1_ZAxis"></label>
                                </div>
                                <div class="form-group">
                                    <label>BoxDim 1</label>
                                    <label>XAxis<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_2_XAxis" id="gomc_config_input_BoxDim_2_XAxis"></label>
                                    <label>YAxis<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_2_YAxis" id="gomc_config_input_BoxDim_2_YAxis"></label>
                                    <label>ZAxis<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_BoxDim_2_ZAxis" id="gomc_config_input_BoxDim_2_ZAxis"></label>
                                </div>
                                <div class="form-group">
                                    <label>CBMC_First</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_CbmcFirst" id="gomc_config_input_CbmcFirst">
                                </div>
                                <div class="form-group">
                                    <label>CBMC_Nth</label>
                                    <input class="form-control" type="number" step="any" min="0"  name="gomc_config_input_CbmcNth" id="gomc_config_input_CbmcNth">
                                </div>
                                <div class="form-group">
                                    <label>CBMC_Ang</label>
                                    <input class="form-control" type="number" step="any" min="0" name="gomc_config_input_CbmcAng" id="gomc_config_input_CbmcAng">
                                </div>
                                <div class="form-group">
                                    <label>CBMC_Dih</label>
                                    <input class="form-control" type="number"  step="any" min="0" name="gomc_config_input_CbmcDih" id="gomc_config_input_CbmcDih">
                                </div>
                            <h4>Please Enter Output Header Format</h4>
                                <div class="form-group">
                                    <label>OutputName</label>
                                    <input class="form-control" type="text" pattern="[\s-]" name="gomc_config_input_OutputName" id="gomc_config_input_OutputName">
                                </div>
                                <label>CoordinatesFreq</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_CoordinatesFreq_Enabled" id="gomc_config_input_CoordinatesFreq_Enabled_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_CoordinatesFreq_Enabled" id="gomc_config_input_CoordinatesFreq_Enabled_false" value="false">
                                        False
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>Value<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_CoordinatesFreq_Value" id="gomc_config_input_CoordinatesFreq_Value"></label>
                                </div>
                                <label>RestartFreq</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_RestartFreq_Enabled" id="gomc_config_input_RestartFreq_Enabled_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_RestartFreq_Enabled" id="gomc_config_input_RestartFreq_Enabled_false" value="false">
                                        False
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>Value<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_RestartFreq_Value" id="gomc_config_input_RestartFreq_Value"></label>
                                </div>
                                <label>ConsoleFreq</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_ConsoleFreq_Enabled" id="gomc_config_input_ConsoleFreq_Enabled_true" value="true">
                                        True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_ConsoleFreq_Enabled" id="gomc_config_input_ConsoleFreq_Enabled_false" value="false">
                                        False
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>Value<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_ConsoleFreq_Value" id="gomc_config_input_ConsoleFreq_Value"></label>
                                </div>
                                <label>BlockAverageFreq</label>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_BlockAverageFreq_Enabled" id="gomc_config_input_BlockAverageFreq_Enabled_true" value="true">True
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input class="form-check-input" type="radio" name="gomc_config_input_BlockAverageFreq_Enabled" id="gomc_config_input_BlockAverageFreq_Enabled_false" value="false">
                                        False
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label>Value<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_BlockAverageFreq_Value" id="gomc_config_input_BlockAverageFreq_Value"></label>
                                </div>
                                <label>HistogramFreq</label>
                                <div class="radio">
                                    <label class="form-check-label"><input class="form-check-input" type="radio" name="gomc_config_input_HistogramFreq_Enabled" id="gomc_config_input_HistogramFreq_Enabled_true" value="true">True</label>
                                </div>
                                <div class="radio">
                                    <label class="form-check-label"><input class="form-check-input" type="radio" name="gomc_config_input_HistogramFreq_Enabled" id="gomc_config_input_HistogramFreq_Enabled_false" value="false">False</label>
                                </div>
                                <div class="form-group">
                                    <label>Value<input class="form-control" type="number" step="any" min="0" name="gomc_config_input_HistogramFreq_Value" id="gomc_config_input_HistogramFreq_Value"></label>
                                </div>
                                <button class="btn btn-success btn-sm pull-left prev-btn">Previous</button>
                                <button class="btn btn-success btn-sm pull-right next-btn">Next</button>
                            </div>
                        </div>
                        <div class='panel panel-success panel-fourth not-render'>
                            <div class='panel-heading '>Output Body Configuration</div>
                            <div class='panel-body '>
                                <h4>Please Enter the Output Body Format</h4>
                                <div class="form-group">
                                    <label>DistName</label>
                                    <input class="form-control" type="text" pattern="[\s-]" name="gomc_config_input_DistName" id="gomc_config_input_DistName">
                                </div>
