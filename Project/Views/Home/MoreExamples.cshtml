@model Project.Models.DownloadsModel.ExampleItem[]
@{
    Layout = "~/Views/Home/_Home_Layout.cshtml";
    ViewBag.Title = "GOMC: Ex-Examples";
}
@section NavSection {
    <li>@Html.ActionLink("GOMC", "Gomc", "Home")</li>
    <li>@Html.ActionLink("Features", "Features", "Home")</li>
    <li>@Html.ActionLink("Downloads", "Downloads", "Home", htmlAttributes: new { id = "onDownloadsPage" })</li>
    <li>@Html.ActionLink("Documentation", "Documentation", "Home")</li>
    <li>@Html.ActionLink("Publications", "Publications", "Home")</li>
    <li>@Html.ActionLink("About", "About", "Home")</li>
}
<main>
    <div class="row">
        <div class="well well-sm col-md-6">
            @foreach (var i in Model)
            {
                <h3>Examples set: @i.TagName</h3>
                <ul>
                    <li class="model-link"><a href="@i.ZipBall">Zip Download</a></li>
                    <li class="model-link"><a href="@i.TarBall">Tar Download</a></li>
                </ul>
            }
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <a>@Html.ActionLink("Click here to go back to most recent example", "Downloads", "Home")</a>
        </div>
    </div>
</main>
