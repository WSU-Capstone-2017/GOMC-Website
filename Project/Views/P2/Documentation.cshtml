                                        <label>ParametersFileName</label>
                                        <input class="form-control" type="text" name="gomc_config_input_ParametersFileName" id="gomc_config_input_ParametersFileName">
                                    </div>
                                    <div class="form-group">
                                        <label>Coordinates</label>
                                        <input class="form-control" type="text" name="gomc_config_input_Coordinates_1" id="gomc_config_input_Coordinates_1">
                                        <input class="form-control" type="text" name="gomc_config_input_Coordinates_2" id="gomc_config_input_Coordinates_2">
                                    </div>
                                    <div class="form-group">
                                        <label>Structures</label>
