<header>
    <h1>Documentation</h1>
</header>
<hr class='grand-line ' />
<main>
    <div class='container-fluid '>
        <section>
            <div class="row">
                <div class='col-md-12 '>
                    <span id="fromLatexConversion">This is where the text from the LaTeX conversion will render</span>
                </div>
                <div class='col-md-12 '>
                    <p>But a link will be provided here in case the user wants to see the PDF: <a href='~/Content/img/Manual.pdf' target="_blank">here</a> </p>
                </div>
            </div>
        </section>
        <section>
            <div class="row">
                <div class='col-md-12 '>
                    <h2>Tutorials</h2>
                </div>
                <div class='col-md-4 '>
                    <img class='img-responsive img-circle ' src='~/Content/img/placeholder.jpg' />
                    <p class='center-p '>Tutorial A</p>
                </div>
                <div class='col-md-4 '>
                    <img class='img-responsive img-circle ' src='~/Content/img/placeholder.jpg' />
                    <p class='center-p '>Tutorial B</p>
                </div>
                <div class='col-md-4 '>
                    <img class='img-responsive img-circle ' src='~/Content/img/placeholder.jpg' />
                    <p class='center-p '>Tutorial C</p>
                </div>
            </div>
            <h3>For more info check us out on youtube<a href='# '> here</a></h3>
        </section>
        <hr class='grand-line ' />
        <section>
            <div class="row">
                <div class='col-md-12 '>
                    <h2>XML Generator</h2>
                    <p class='center-p '>Click the button below to fill out a form for conversion into xml</p>
                    <div class='center-p '>
                        <button class='btn btn-large btn-primary ' id='xmlTrigger'>Start</button>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class='col-md-12 hidden-until well well-lg ' id=xmlDataContainer>
                    <div class="progress">
                        <div class='progress-bar progress-bar-striped active bar-style' role='progressbar' aria-valueactive='1' aria-valuemin='0' aria-valuemax="8" id='userProgress'>
                        </div>
                    </div>
                    <form id="ConfigInput_TestInput_Form">
                        <div class='panel-group '>
                            <div class="panel panel-info panel-first">
                                <div class="panel-heading">Please select an Ensemble</div>
                                <div class="panel-body">
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_Nvt" value="Nvt">
                                            Nvt
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_Npt" value="Npt">
                                            Npt
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_Gcmc" value="Gcmc">
                                            Gcmc
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_GibbsNvt" value="GibbsNvt">
                                            GibbsNvt
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Ensemble" id="gomc_config_input_Ensemble_GibbsNpt" value="GibbsNpt">
                                            GibbsNpt
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-success panel-second">
                                <div class="panel-heading">Please enter the following input data</div>
                                <div class="panel-body">
                                    <label>Restart</label>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Restart" id="gomc_config_input_Restart_true" value="true">
                                            True
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Restart" id="gomc_config_input_Restart_false" value="false">
                                            False
                                        </label>
                                    </div>
                                    <label>Prng</label>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Prng" id="gomc_config_input_Prng_Random" value="Random">RANDOM
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_Prng" id="gomc_config_input_Prng_Intseed" value="Intseed">INTSEED
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label>RandomSeed</label>
                                        <input class="form-control" type="number" name="gomc_config_input_RandomSeed" id="gomc_config_input_RandomSeed">
                                    </div>
                                    <label>ParaType</label>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_ParaType" id="gomc_config_input_ParaType_Charmm" value="Charmm">
                                            CHARMM
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_ParaType" id="gomc_config_input_ParaType_Exotic" value="Exotic">
                                            EXOTIC
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input class="form-check-input" type="radio" name="gomc_config_input_ParaType" id="gomc_config_input_ParaType_Martini" value="Martini">
                                            MARTINI
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class='panel panel-default panel-third'>
