@section scripts
{
    <script>
        var frm = $("#ConfigInput_TestInput_Form");
        var frmBtn = $("#api_ConfigInput_TestInput_Submit");
        frmBtn.click(function() {
            $.post('/api/configinput/FormPost', frm.serialize())
                .done(function(data) {
                    var newUrl = '/api/configinput/DownloadFromGuid?guid=' + data;
                    window.location.replace(newUrl);
                })
                .fail(function(jqXhR) {
                    $("#gomc_config_input_error").html(JSON.parse(jqXhR.responseText)["Message"]);
                });
        });
    </script>
}
