                                    </div>
                                    <label>Fugacity</label>
                                    <div class="form-group">
                                        <label>ResName<input class="form-control" type="text" name="gomc_config_input_Fugacity_ResName" id="gomc_config_input_Fugacity_ResName"></label>
                                        <label>Value<input class="form-control" type="number" name="gomc_config_input_Fugacity_Value" id="gomc_config_input_Fugacity_Value"></label>
                                    </div>
